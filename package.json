{
  "name": "gulp-base",
  "version": "0.0.3",
  "description": "A base for projects using Gulp",
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/rthor/gulp-base.git"
  },
  "author": {
    "name": "Ragnar Þór Valgeirsson (rthor)",
    "email": "ragnar.valgeirsson@gmail.com",
    "url": "http://rthor.is"
  },
  "bugs": {
    "url": "https://github.com/rthor/gulp-base/issues"
  },
  "homepage": "https://github.com/rthor/gulp-base",
  "license": "MIT",
  "devDependencies": {
    "bower": "~1.3.3",
    "gulp-bower-files": "~0.2.2",
    "browserify": "~3.44.2",
    "browserify-shim": "~3.4.1",
    "glob": "~3.2.9",
    "gulp": "~3.6.2",
    "gulp-autoprefixer": "0.0.7",
    "gulp-clean": "~0.2.4",
    "gulp-connect": "~2.0.5",
    "gulp-csso": "~0.2.8",
    "gulp-filter": "~0.4.1",
    "gulp-jade": "~0.5.0",
    "gulp-notify": "~1.2.5",
    "gulp-open": "~0.2.8",
    "gulp-plumber": "~0.6.1",
    "gulp-ruby-sass": "~0.4.3",
    "gulp-rename": "~1.2.0",
    "gulp-util": "~2.2.14",
    "vinyl-source-stream": "~0.1.1"
  },
  "dependencies": {
    "jquery": "~2.1.0"
  }
}
