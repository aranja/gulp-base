{
  "name": "gulp-base",
  "version": "0.0.4",
  "description": "A base for projects using Gulp",
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/aranja/gulp-base.git"
  },
  "author": {
    "name": "Aranja",
    "email": "aranja@aranja.com",
    "url": "http://aranja.com"
  },
  "bugs": {
    "url": "https://github.com/aranja/gulp-base/issues"
  },
  "homepage": "https://github.com/aranja/gulp-base",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^8.1.0",
    "browserify-shim": "^3.8.2",
    "glob": "^4.3.2",
    "gulp": "^3",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-changed": "^1.1.0",
    "gulp-connect": "^2.2.0",
    "gulp-csso": "^0.2.9",
    "gulp-eslint": "^0.2.0",
    "gulp-imagemin": "^2.1.0",
    "gulp-jade": "^0.10.0",
    "gulp-jscs": "^1.4.0",
    "gulp-less": "^2.0.1",
