    "gulp-plumber": "^0.6.6",
    "gulp-rename": "^1.2.0",
    "gulp-util": "^3.0.2",
    "gulp-watch": "^3.0.0",
    "rimraf": "^2.2.8",
    "run-sequence": "^1.0.2",
    "vinyl-source-stream": "^1.0.0"
